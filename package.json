{
  "name": "@contactlab/appy",
  "version": "5.2.0",
  "description": "A functional wrapper around Fetch API",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "module-sync": "./dist/_es6/index.js",
      "module": "./dist/_es6/index.js"
    },
    "./request": {
      "require": "./dist/request.js",
      "types": "./dist/request.d.ts",
      "module-sync": "./dist/_es6/request.js",
      "module": "./dist/_es6/request.js"
    },
    "./response": {
      "require": "./dist/response.js",
      "types": "./dist/response.d.ts",
      "module-sync": "./dist/_es6/response.js",
      "module": "./dist/_es6/response.js"
    },
    "./combinators/abort": {
      "require": "./dist/combinators/abort.js",
      "types": "./dist/combinators/abort.d.ts",
      "module-sync": "./dist/_es6/combinators/abort.js",
      "module": "./dist/_es6/combinators/abort.js"
    },
    "./combinators/body": {
      "require": "./dist/combinators/body.js",
      "types": "./dist/combinators/body.d.ts",
      "module-sync": "./dist/_es6/combinators/body.js",
      "module": "./dist/_es6/combinators/body.js"
    },
    "./combinators/decoder": {
      "require": "./dist/combinators/decoder.js",
      "types": "./dist/combinators/decoder.d.ts",
      "module-sync": "./dist/_es6/combinators/decoder.js",
      "module": "./dist/_es6/combinators/decoder.js"
    },
    "./combinators/headers": {
      "require": "./dist/combinators/headers.js",
      "types": "./dist/combinators/headers.d.ts",
      "module-sync": "./dist/_es6/combinators/headers.js",
      "module": "./dist/_es6/combinators/headers.js"
    },
    "./combinators/method": {
      "require": "./dist/combinators/method.js",
      "types": "./dist/combinators/method.d.ts",
      "module-sync": "./dist/_es6/combinators/method.js",
      "module": "./dist/_es6/combinators/method.js"
    },
    "./combinators/url-params": {
      "require": "./dist/combinators/url-params.js",
      "types": "./dist/combinators/url-params.d.ts",
      "module-sync": "./dist/_es6/combinators/url-params.js",
      "module": "./dist/_es6/combinators/url-params.js"
    }
  },
  "sideEffects": false,
  "author": "Contactlab",
  "license": "Apache-2.0",
  "homepage": "https://contactlab.github.io/appy",
  "bugs": "https://github.com/contactlab/appy/issues",
  "repository": "contactlab/appy",
  "keywords": [
    "contactlab",
    "appy",
    "fetch",
    "fp",
    "fp-ts",
    "typescript"
  ],
  "engines": {
    "node": ">= 16.0",
    "npm": ">= 8.0"
  },
  "scripts": {
    "prepare": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json",
    "check": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pretest": "npm run check && npm run lint",
    "test": "jest --runInBand",
    "prebuild": "rm -rf ./dist",
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json",
    "postbuild": "ts-node scripts/prepare-dist",
    "prepublishOnly": "ts-node scripts/pre-publish",
    "prerelease": "npm test && npm run build",
    "release": "ts-node scripts/release",
    "docs": "docs-ts",
    "docs:update": "npm run docs && git add docs",
    "postdocs": "ts-node scripts/docs"
  },
  "peerDependencies": {
    "fp-ts": "^2.8.0"
  },
  "devDependencies": {
    "@types/fetch-mock": "^7.3.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.7",
    "abort-controller": "^3.0.0",
    "cross-fetch": "^4.0.0",
    "docs-ts": "^0.8.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^9.0.0",
    "fetch-mock": "^9.11.0",
    "fp-ts": "^2.8.0",
    "husky": "^8.0.0",
    "io-ts": "^2.2.13",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "lint-staged": "^15.0.1",
    "prettier": "^2.2.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  }
}
